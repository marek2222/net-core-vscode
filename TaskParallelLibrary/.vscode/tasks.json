{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "run",
			"command": "dotnet",
			"type": "process",
			"args": ["run", "${workspaceFolder}/TaskParallelLibrary.csproj"],
			"problemMatcher": "$tsc"
		},
		{
			"label": "build",
			"command": "dotnet",
			"type": "process",
			"args": ["build", "${workspaceFolder}/TaskParallelLibrary.csproj"],
			"problemMatcher": "$tsc"
		},
		{
			"label": "publish",
			"command": "dotnet",
			"type": "process",
			"args": ["publish", "${workspaceFolder}/TaskParallelLibrary.csproj"],
			"problemMatcher": "$tsc"
		},
		{
			"label": "watch",
			"command": "dotnet",
			"type": "process",
			"args": ["watch", "run", "${workspaceFolder}/TaskParallelLibrary.csproj"],
			"problemMatcher": "$tsc"
		}
	]
}
